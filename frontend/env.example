# Environment Variables for Monte Carlo Platform Frontend
# Copy this file to .env and fill in the actual values

# API Configuration
VITE_API_URL=http://localhost:8000/api
VITE_DEMO_API_KEY=your_demo_api_key_here

# Demo Authentication (for development only)
VITE_DEMO_USERNAME=demo_user
VITE_DEMO_PASSWORD=secure_demo_password

# Auth0 Configuration (if using Auth0)
VITE_AUTH0_DOMAIN=your-auth0-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your_auth0_client_id
VITE_AUTH0_AUDIENCE=your_api_identifier

# External Services
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key

# Security Settings
VITE_ENABLE_CONSOLE_PROTECTION=true
VITE_ENABLE_DEVTOOLS_DETECTION=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error

# WARNING: Never commit .env files with real secrets to version control
# Add .env to your .gitignore file
